#include <stdio.h>
int main() {
    int n1, n2, i, j, k;
    printf("Enter size of array 1: ");
    scanf("%d", &n1);
    int a1[n1];
    for(i = 0; i < n1; i++) scanf("%d", &a1[i]);
    printf("Enter size of array 2: ");
    scanf("%d", &n2);
    int a2[n2], merge[n1+n2];
    for(i = 0; i < n2; i++) scanf("%d", &a2[i]);
    for(i = 0; i < n1; i++) merge[i] = a1[i];
    for(j = 0; j < n2; j++) merge[i++] = a2[j];
    printf("Merged array: ");
    for(k = 0; k < n1+n2; k++) printf("%d ", merge[k]);
    printf("\n");
    return 0;
}
