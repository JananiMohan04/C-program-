#include <stdio.h>

int main() {
    int n, i;
    float price, quantity, total = 0;

    printf("Enter number of items: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        printf("Enter price of item %d: ", i);
        scanf("%f", &price);
        printf("Enter quantity of item %d: ", i);
        scanf("%f", &quantity);
        total += price * quantity;
    }

    printf("Total bill amount: â‚¹%.2f\n", total);

    return 0;
}
