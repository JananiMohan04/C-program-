#include <stdio.h>
int main() {
    int n;
    printf("Enter array size: ");
    scanf("%d", &n);
    int arr1[n], arr2[n];
    printf("Enter %d elements: ", n);
    for(int i=0; i<n; i++) {
        scanf("%d", &arr1[i]);
        arr2[i] = arr1[i];
    }
    printf("Original: ");
    for(int i=0; i<n; i++) printf("%d ", arr1[i]);
    printf("\nCopied: ");
    for(int i=0; i<n; i++) printf("%d ", arr2[i]);
    printf("\n");
    return 0;
}
