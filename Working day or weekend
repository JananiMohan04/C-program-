#include <stdio.h>

int main() {
    int day;

    printf("Enter day number (1-7, Mon=1, Sun=7): "number (1-7, Mon=1, Sun=7): ");
    scanf("%d", &day);

    if (day >= 1 && day <= 5)
        printf("Working day \n");
    else if (day == 6 || day == 7)
        printf("Weekend\n");
    else
        printf("Invalid day\n");

    return 0;
}
