#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main() {
    char str[100];
    printf("Enter string: ");
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")] = 0;
    int i = 0, j = strlen(str) - 1;
    while(i < j) {
        if(str[i] != str[j]) {
            printf("Not palindrome\n");
            return 0;
        }
        i++; j--;
    }
    printf("Palindrome\n");
    return 0;
}
