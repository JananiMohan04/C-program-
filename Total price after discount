#include <stdio.h>

int main() {
    float price, discount, total;

    printf("Enter original price: ₹");
    scanf("%f", &price);
    printf("Enter discount %%: ");
    scanf("%f", &discount);

    total = price - (price * discount / 100);
    printf("Price after discount: ₹%.2f\n", total);
    return 0;
}
